{% if tenantManagers %}
    <tr>
        <td id="title-tenant-managers">
            <p>{{ 'tenant_managers.title'|trans }}</p>
        </td>
        <td>
            {% for tenantManager in tenantManagers %}
                <div class="checkbox-tenant-managers {% if tenantManager.tenant and tenantManager.tenant.id != tenant.id %} disabled {% endif %}">
                    <input type="checkbox"
                           {% if tenantManager.tenant and tenantManager.tenant.id == tenant.id %}checked="checked"
                            {% elseif tenantManager.tenant and tenantManager.tenant.id is not null and tenantManager.tenant.id != tenant.id %}
                                disabled class="disabled"
                            {% endif %}
                           name="tenantData[manager][{{ tenantManager.id }}]" id="id-manager-{{ tenantManager.id }}">
                    <label for="id-manager-{{ tenantManager.id }}">
                        <i class="far fa-square"></i>
                        <i class="far fa-check-square"></i>
                    </label>
                    <label for="id-manager-{{ tenantManager.id }}">{{ tenantManager.fullName }}</label>
                </div>
            {% endfor %}
        </td>
    </tr>
{% endif %}